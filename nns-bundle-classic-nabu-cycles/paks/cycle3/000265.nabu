PPROP ".SYSTEM "BURY "TRUE
TO DRAGON 
TEXTMODE LOADCHAR "DRAGON
SETBG 4 SETTC 15 CLEARTEXT
DR 6225 39 1
DR 6303 20 40
DR 6265 20 40
DR 7065 39 1
MAKE "X 6305 REPEAT 6 [REPEAT 2 [DR :X 39 1 MAKE "X :X + 1] MAKE "X :X + 40]
DR :X 39 1
MAKE "X 6266 REPEAT 13 [REPEAT 19 [.VDEPOSIT :X 131 MAKE "X :X + 40] MAKE "X :X - 757]
MAKE "V 0 MAKE "W 0 MAKE "H 30 REPARSE RECYCLE INIT GAME
END

TO INIT 
SETBG 4 MAKE "O 0 MAKE "Y -40 MAKE "Z 138 MAKE "L -40 MAKE "K 134 MAKE "R 131 MAKE "S 6284 MAKE "D 7004 MAKE "G 0 MAKE "P 0 MAKE "TI 0 MAKE "U 0
END

TO SOUND :E
.AUDIOWRITE 7 55 .AUDIOWRITE 8 :E .AUDIOWRITE 6 31
END

TO TIME 
MAKE "TI :TI + 0.3
IF NODES < 20 [REPARSE RECYCLE]
SETCURSOR [0 0] PR SE [TIME:] INT :TI
SETCURSOR [20 0] PR SE [SCORE:] :W
SETCURSOR [15 1] PR SE [HIGH SCORE:] :V
END

TO PRIZE 
IF :A = 1 [MAKE "M 40 MAKE "C 139 MAKE "G 131 STOP]
IF :A = 2 [MAKE "M -40 MAKE "C 139 MAKE "G 131 STOP]
IF :A = 3 [MAKE "M -1 MAKE "C 141 MAKE "G 160 STOP]
IF :A = 4 [MAKE "M 1 MAKE "C 140 MAKE "G 160 STOP]
END

TO GAME 
LABEL "GAME MAKE "A (RANDOM 4) + 1 MAKE "B (RANDOM 3) + 1 MAKE "B :B * 3
LABEL "PRIZE PRIZE IF .VEXAMINE (:S + :M) = 130 [GO "GAME]
.VDEPOSIT :S :G MAKE "S :S + :M .VDEPOSIT :S :C MAKE "ST JOY 1
IF FIREP 1 [MAKE "I :D + :L GO "BTTN]
LABEL "STICK STICK
IF OR :ST = 1 :ST = 3 [GO "MTN]
IF OR :ST = 5 :ST = 7 [GO "MTN]
MAKE "Y 0
LABEL "MTN
IF .VEXAMINE (:D + :Y) = 130 [MAKE "Y 0]
IF OR :Y = 1 :Y = -1 [MAKE "F 160] [MAKE "F 131]
.VDEPOSIT :D :F MAKE "D :D + :Y .VDEPOSIT :D :Z TIME
IF OR :TI = :H :TI > :H [FINISH]
IF OR :TI = :H - 5 :TI > :H - 5 [SETBG 6]
MAKE "B :B - 1 IF :B = 0 [GO "GAME] [GO "PRIZE]
LABEL "BTTN IF .VEXAMINE :I = 130 [GO "BRICK]
.VDEPOSIT :I :K MAKE "O :O + 1 SOUND :O IF :O = 10 [GO "FIRE]
MAKE "I :I + :L IF :I = :S [GO "MUNCH] [GO "BTTN]
LABEL "BRICK
IF AND .VEXAMINE :I = 130 :O = 0 [GO "STICK]
MAKE "I :I - :L
LABEL "FIRE
.VDEPOSIT :I :R MAKE "I :I - :L
MAKE "O :O - 1 SOUND :O
IF AND :O = 0 :U = 1 [.VDEPOSIT :D :R .AUDIOWRITE 7 63 MAKE "U 0 INIT GO "GAME]
IF :O = 0 [GO "STICK] [GO "FIRE]
LABEL "MUNCH
.VDEPOSIT :I 128 MAKE "W :W + (:H - INT :TI) MAKE "H :H - 5
IF :H = 5 [MAKE "H 30]
MAKE "U 1 MAKE "O :O + 1 GO "FIRE
END

TO STICK 
IF :ST = 1 [MAKE "Y -40 MAKE "L -40 MAKE "K 134 MAKE "Z 138 MAKE "R 131 STOP]
IF :ST = 3 [MAKE "Y 1 MAKE "L 1 MAKE "K 132 MAKE "Z 136 MAKE "R 160 STOP]
IF :ST = 5 [MAKE "Y 40 MAKE "L 40 MAKE "K 135 MAKE "Z 129 MAKE "R 131 STOP]
IF :ST = 7 [MAKE "Y -1 MAKE "L -1 MAKE "K 133 MAKE "Z 137 MAKE "R 160 STOP]
END

TO DR :X1 :R1 :S1
REPEAT :R1 [.VDEPOSIT :X1 130 MAKE "X1 :X1 + :S1]
MAKE "X :X1
END

TO FINISH 
.VDEPOSIT :D :F .VDEPOSIT :S :G
IF :W > :V [MAKE "V :W]
SETCURSOR [14 0] PR [GAME OVER] WAIT 200
SETCURSOR [0 0] PR [PRESS ANY KEY TO PLAY]
MAKE "Q RC MAKE "W 0 MAKE "H 30
SETBG 4 INIT GAME
END

