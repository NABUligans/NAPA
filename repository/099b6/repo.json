[
  {
    "name": "Classic NABU Network Cycles",
    "manifest": {
      "paks": [
        {
          "path": "cycle1",
          "name": "1984 Cycle 1"
        },
        {
          "path": "cycle2",
          "name": "1984 Cycle 2"
        },
        {
          "name": "Cycle Direct",
          "options": {
            "exploitloader": true
          },
          "path": "direct"
        }
      ]
    },
    "id": "nns-bundle-classic-nabu-cycles",
    "path": "nns-bundle-classic-nabu-cycles.napa",
    "version": "1.0.1",
    "author": "The NABU Network",
    "description": "The classic 1984 NABU Network experience.\nAND Cycle Direct: skip the menu! \n"
  },
  {
    "name": "Ishkur CPM",
    "manifest": {
      "programs": [
        {
          "path": "NFS_HYBRID_BOOT.nabu",
          "name": "Ishkur CPM NFS"
        },
        {
          "path": "NDSK_HYBRID_BOOT.nabu",
          "name": "Ishkur CPM NDSK"
        },
        {
          "path": "IDE_BOOT.nabu",
          "name": "Ishkur CPM IDE (Experimental)"
        }
      ],
      "storage": [
        {
          "options": {
            "updatetype": "copy"
          },
          "path": "NDSK_A.IMG"
        },
        {
          "options": {
            "updatetype": "copy"
          },
          "path": "NDSK_B.IMG"
        }
      ]
    },
    "features": {
      "NHACPv01": true
    },
    "url": "https://github.com/tergav17/IshkurCPM",
    "id": "nns-bundle-ishkurcpm",
    "path": "nns-bundle-ishkurcpm.napa",
    "version": "0.1.2307291014",
    "author": "teragav17",
    "description": "Ishkur is a flavor of CPM 2.2 made for the NABU from the ground up. System files are loaded via NHACP. \nThis is the 'hybrid' flavor, and supports real drives (C & D) as well as cloud disks (A,B,E,etc).\n"
  },
  {
    "features": {
      "retronet": true
    },
    "version": "1.0.3",
    "url": "https://www.nabu.ca",
    "id": "nns-bundle-nabuca",
    "manifest": {
      "sources": [
        {
          "name": "NABU.ca",
          "options": {
            "retronetserver": true,
            "retronet": true
          },
          "path": "https://cloud.nabu.ca/HomeBrew/titles/filesv2.txt"
        }
      ],
      "storage": [
        {
          "options": {
            "updatetype": "copy"
          },
          "path": "CCPM"
        },
        {
          "options": {
            "updatetype": "copy"
          },
          "path": "C.dsk"
        }
      ]
    },
    "path": "nns-bundle-nabuca.napa",
    "name": "NABU.ca",
    "author": "NABU.ca",
    "description": "Support for the NABU.ca feed and files for Cloud CP/M\n"
  },
  {
    "version": "1.0.4.5",
    "url": "https://nabunetwork.com",
    "id": "nns-bundle-nabunetworkcom",
    "manifest": {
      "sources": [
        {
          "path": "https://www.nabunetwork.com/NabuNetwork.xml",
          "name": "NabuNetwork.com"
        }
      ],
      "paks": [
        {
          "name": "NabuNetwork.com Headless",
          "options": {
            "headlessmenu": true
          },
          "path": "headless"
        }
      ]
    },
    "path": "nns-bundle-nabunetworkcom.napa",
    "name": "NabuNetwork.com",
    "author": "NabuNetwork.com",
    "description": "Support for the NabuNetwork.com feed.\n"
  },
  {
    "name": "Production Dave's Nabu Games",
    "manifest": {
      "programs": [
        {
          "path": "invaders.nabu",
          "name": "Invaders"
        },
        {
          "path": "snake.nabu",
          "name": "Snake"
        },
        {
          "path": "tetris.nabu",
          "name": "Tetris"
        }
      ]
    },
    "url": "https://github.com/linuxplayground/nabu-games",
    "id": "nns-bundle-productiondave-nabugames",
    "path": "nns-bundle-productiondave-nabugames.napa",
    "version": "3.5.3",
    "author": "Production Dave",
    "description": "A collection of games for the Nabu (and Z80 Retro) by Production Dave.\n"
  }
]
